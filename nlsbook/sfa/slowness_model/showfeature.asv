function showfeature(D,n)
% each row of D is a frame represented by a double vector.
% note that D must be in the same scale as the training data.
global SFA_STRUCTS;
addpath ../sfa_tk/sfa
addpath ../sfa_tk/lcov
load simulation_results.mat;
SFA_STRUCTS{1}=strct;
y=sfa_execute(1,D);
x=1:100;
plot(x,y(:,1:3));

legend('1','2','3');
 